.classifier
  width 100%
  height 100%
  background white
  .classifier_content
    width 100%
    height 100%
    .classifier_header // 头部
      width 100%
      height 35px
      font-size 15px
      border-bottom 1px solid #d9d9d9
      .search
        width 90%
        height 100%
        margin 7px 0 0 19px
        .search_content
          width 100%
          height 27px
          background #ededed
          border-radius 5px
          line-height 27px
          color #333
          .iconfont
            display inline-block
            margin-left  23%
          span
            display inline-block
    .classifier_left
      width 21.5%
      height 100%
      display inline-block
      border-right 1px solid #d9d9d9
      vertical-align top
      .lists
        width 100%
        height 100%
        li
          width 100%
          height 25px
          text-align center
          margin-top 19px
          display block
          font-size 14px
          color #333
          letter-spacing 1px
          &.active
            border-left 3px solid #BE464A
            font-size 15px
